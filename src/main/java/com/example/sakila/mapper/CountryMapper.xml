<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.sakila.mapper.CountryMapper"><!-- 인터페이스 풀네임 적는곳 -->
	<select id="selectCountryList"
		resultType="com.example.sakila.vo.Country">
		SELECT country_id countryId , country , last_update lastUpdate
		FROM country
	</select><!-- 어떤 메소드와 연결할건지 적는곳 -->
</mapper>